{
	"name": "sinpro-dev",
	"version": "0.67.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "tsx ./server/index.ts",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"test:run": "vitest run",
		"test:e2e": "playwright test",
		"test:ci": "CI=true npm run test",
		"coverage:ci": "CI=true npm run coverage",
		"test:e2e:ci": "CI=true npm run test:e2e",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettie --write .",
		"prepare": "husky install",
		"typecheck": "tsc --noEmit",
		"typecheck:why": "tsc --noEmit --explainFiles > explainTypes.txt",
		"search-index": "tsx ./src/lib/docs/search_index.ts",
		"branch": "git checkout main && git pull && tsx ./src/lib/git/create_git_branch.ts",
		"prebuild": "npm run search-index"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.36.2",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.22.3",
		"@types/glob": "^8.1.0",
		"@types/highlight.js": "^10.1.0",
		"@types/jsdom": "^21.1.1",
		"@types/markdown-it-link-attributes": "^3.0.1",
		"@types/morgan": "^1.9.4",
		"@types/nprogress": "^0.2.0",
		"@types/prettier": "^2.7.3",
		"@types/remove-markdown": "^0.3.1",
		"@types/vivus": "^0.4.4",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"@vitest/coverage-c8": "^0.33.0",
		"@vitest/coverage-v8": "^0.34.1",
		"autoprefixer": "^10.4.14",
		"dotenv": "^16.3.1",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.32.4",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.3",
		"postcss": "^8.4.27",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.1",
		"prettier-plugin-tailwindcss": "^0.4.1",
		"remove-markdown": "^0.5.0",
		"stylelint": "^15.10.2",
		"stylelint-config-standard": "^34.0.0",
		"stylelint-config-standard-scss": "^10.0.0",
		"svelte": "^4.1.1",
		"svelte-check": "^3.4.6",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"tailwindcss-dir": "^4.0.0",
		"tslib": "^2.6.1",
		"tsx": "^3.12.7",
		"typescript": "^5.1.6",
		"user-agent-data-types": "^0.3.1",
		"vite": "^4.4.7",
		"vitest": "^0.33.0"
	},
	"type": "module",
	"dependencies": {
		"@types/markdown-it": "^12.2.3",
		"fuse.js": "^6.6.2",
		"glob": "^10.3.3",
		"gray-matter": "^4.0.3",
		"highlight.js": "^11.8.0",
		"jsdom": "^22.1.0",
		"markdown-it": "^13.0.1",
		"markdown-it-highlightjs": "^4.0.1",
		"markdown-it-link-attributes": "^4.0.1",
		"nprogress": "^0.2.0",
		"svelte-local-storage-store": "^0.5.0",
		"svelte-meta-tags": "^2.8.0",
		"vivus": "^0.4.6",
		"winston": "^3.10.0",
		"winston-console-format": "^1.0.8",
		"winston-daily-rotate-file": "^4.7.1"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": "eslint --cache --fix",
		"*.css": "stylelint --fix",
		"*.{js,css,md,ts,svelte,css,scss,json}": "prettier --write"
	}
}
